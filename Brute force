class Main {
    public static void main(String[] args) {
        int arr[]={2,1,5,3,1,2};
        int target=7;
        for(int i=0;i<arr.length;i++){
            for(int j=i+1;j<arr.length;j++){
                if(target==arr[i]+arr[j]){
                    System.out.println("found");
                }
            }
        }
        
    }
}
