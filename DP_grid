#include <stdio.h>

int main() {
    
    int m = 3;   // number of rows
    int n = 3;   // number of columns
    
    int dp[3][3];   // DP table
    
    // Step 1: Fill first column with 1
    for(int i = 0; i < m; i++) {
        dp[i][0] = 1;  // Only one way (all down)
    }
    
    // Step 2: Fill first row with 1
    for(int j = 0; j < n; j++) {
        dp[0][j] = 1;  // Only one way (all right)
    }
    
    // Step 3: Fill remaining cells
    for(int i = 1; i < m; i++) {
        for(int j = 1; j < n; j++) {
            
            dp[i][j] = dp[i-1][j] + dp[i][j-1]; 
            // from top + from left
            
        }
    }
    
    printf("Total Unique Paths = %d", dp[m-1][n-1]);
    
    return 0;
}
